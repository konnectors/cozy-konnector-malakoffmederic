{
  "name": "cozy-konnector-malakoffmederic",
  "version": "1.0.0",
  "description": "Import your Malakoff Mederic bills",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/konnectors/cozy-konnector-malakoffmederic.git"
  },
  "keywords": [],
  "author": "",
  "license": "AGPL-3.0",
  "main": "src/index.js",
  "eslintConfig": {
    "extends": [
      "eslint-config-cozy-app"
    ]
  },
  "eslintIgnore": [
    "build"
  ],
  "scripts": {
    "standalone": "DEBUG=* cozy-run-standalone src/index.js",
    "standalone:record": "REPLAY=record cozy-run-standalone src/index.js",
    "standalone:replay": "REPLAY=replay cozy-run-standalone src/index.js",
    "dev": "cozy-run-dev src/index.js",
    "build": "webpack",
    "precommit": "eslint .",
    "lint": "eslint --fix .",
    "deploy": "git-directory-deploy --directory build/ --branch ${DEPLOY_BRANCH:-build} --repo=${DEPLOY_REPOSITORY:-https://$GITHUB_TOKEN@github.com/konnectors/cozy-konnector-malakoffmederic.git}",
    "cozyPublish": "cozy-app-publish --token $REGISTRY_TOKEN --build-commit $(git rev-parse ${DEPLOY_BRANCH:-build})"
  },
  "dependencies": {
    "cozy-konnector-libs": "3.8.1",
    "moment": "^2.18.1"
  },
  "devDependencies": {
    "copy-webpack-plugin": "4.5.1",
    "cozy-app-publish": "0.3.6",
    "cozy-jobs-cli": "1.0.18",
    "eslint": "4.19.1",
    "eslint-config-cozy-app": "0.7.0",
    "git-directory-deploy": "1.5.1",
    "husky": "0.14.3",
    "pretty-quick": "1.4.1",
    "webpack": "4.7.0",
    "webpack-cli": "2.1.3"
  }
}
