{
  "name": "cozy-konnector-malakoffmederic",
  "version": "1.0.0",
  "description": "Import your Malakoff Mederic bills",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/konnectors/cozy-konnector-malakoffmederic.git"
  },
  "keywords": [],
  "author": "",
  "license": "AGPL-3.0",
  "main": "src/index.js",
  "eslintConfig": {
    "extends": [
      "eslint-config-cozy-app"
    ]
  },
  "eslintIgnore": [
    "build"
  ],
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "scripts": {
    "standalone": "DEBUG=* cozy-konnector-standalone src/index.js",
    "standalone:record": "REPLAY=record cozy-konnector-standalone src/index.js",
    "standalone:replay": "REPLAY=replay cozy-konnector-standalone src/index.js",
    "dev": "cozy-konnector-dev src/index.js",
    "build": "webpack",
    "format": "prettier --write '**/*.{js,json}'",
    "precommit": "eslint .",
    "lint": "eslint .",
    "deploy": "git-directory-deploy --directory build/ --branch build --repo=https://github.com/konnectors/cozy-konnector-malakoffmederic.git",
    "deploy:prod": "git-directory-deploy --directory build/ --branch latest --repo=https://github.com/konnectors/cozy-konnector-malakoffmederic.git"
  },
  "dependencies": {
    "cozy-konnector-libs": "^3.7.3",
    "moment": "^2.18.1"
  },
  "devDependencies": {
    "copy-webpack-plugin": "4.5.1",
    "cozy-jobs-cli": "^1.0.4",
    "eslint": "4.18.2",
    "eslint-config-cozy-app": "^0.4.0",
    "eslint-config-prettier": "2.9.0",
    "git-directory-deploy": "1.5.1",
    "husky": "0.14.3",
    "prettier": "1.11.1",
    "pretty-quick": "1.4.1",
    "webpack": "4.1.1",
    "webpack-cli": "2.0.11"
  }
}
